{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": {
          "version": 3,
          "proposals": true
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "extensible-destructuring",
      {
        "mode": "optout",
        "impl": "immutable"
      }
    ]
  ]
}